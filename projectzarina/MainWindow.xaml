<Window x:Class="projectzarina.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:projectzarina"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid x:Name="Text12">
        <Button Content="Assign Path" HorizontalAlignment="Left" Margin="114,50,0,0" VerticalAlignment="Top" Width="93" Click="assignPath" Height="23"/>
        <TextBox x:Name="TextScreenshotPath" HorizontalAlignment="Left" Height="23" Margin="212,50,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="559"></TextBox>
        <Button x:Name="Button_SaveSettings" Content="Save Path" HorizontalAlignment="Left" Margin="114,90,0,0" VerticalAlignment="Top" Width="93" Height="23" Click="saveSettings"/>
        <Label x:Name="SaveMessage" Content="Data Saved Successfully" HorizontalAlignment="Left" Margin="236,88,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.037,0.309" Width="154" Visibility="Hidden"/>
        <TextBox x:Name="DebugLable" HorizontalAlignment="Left" Height="189" Margin="114,165,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="541" IsEnabled="False"/>
        <Button Content="Logout" HorizontalAlignment="Left" Margin="696,366,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>

    </Grid>
</Window>
